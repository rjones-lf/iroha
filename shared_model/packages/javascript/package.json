{
  "name": "iroha-lib",
  "version": "0.0.3",
  "description": "Modern JavaScript library for Iroha. https://github.com/hyperledger/iroha",
  "main": "index.js",
  "scripts": {
    "build": "node-pre-gyp build",
    "rebuild": "node-pre-gyp rebuild",
    "install": "node-pre-gyp install --fallback-to-build",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepublishOnly": "npm ls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stinger112/iroha-js.git"
  },
  "keywords": [
    "iroha",
    "iroha-js",
    "irohajs",
    "iroha-node",
    "iroha-nodejs",
    "iroha-javascript",
    "iroha-client",
    "iroha-lib"
  ],
  "contributors": [
    {
      "name": "Viacheslav Bikbaev",
      "email": "viacheslav@soramitsu.co.jp"
    },
    {
      "name": "Arseniy Fokin",
      "email": "fokin@soramitsu.co.jp"
    }
  ],
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/hyperledger/iroha/issues"
  },
  "homepage": "https://github.com/hyperledger/iroha#readme",
  "dependencies": {
    "google-protobuf": "^3.5.0",
    "grpc": "^1.9.1",
    "node-pre-gyp": "^0.6.39"
  },
  "devDependencies": {
    "node-pre-gyp-github": "^1.3.1",
    "grpc-tools": "^1.6.6"
  },
  "bundledDependencies": [
    "node-pre-gyp"
  ],
  "directories": {
    "example": "example",
    "lib": "lib"
  },
  "binary": {
    "module_name": "iroha_lib",
    "module_path": "lib/{node_abi}-{platform}-{arch}-{libc}/",
    "host": "https://github.com/stinger112/iroha-js/releases/download/",
    "remote_path": "{version}",
    "package_name": "{node_abi}-{platform}-{arch}-{libc}.tar.gz"
  }
}
